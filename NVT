###NVT
integrator=md
dt=0.002
nsteps=175000
tcoupl=V-rescale
tc-grps=System
tau-t=0.1
ref-t=350
pcoupl=no
constraints=h-bonds
cutoff-scheme=Verlet
coulombtype=PME
rcoulomb=1.0
rvdw=1.0
pbc=xyz
nstxout-compressed=2500
nstenergy=1000
nstlog=1000

gmx grompp -f .\nvt.mdp -c .\em.gro -p .\topol.top -o .\nvt.tpr 2> .\grompp_err.txt
gmx mdrun  -deffnm nvt -gpu_id 0 -ntmpi 1
