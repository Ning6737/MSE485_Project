###prod
integrator=md
dt=0.002
nsteps=500000000    ###simulation time
tcoupl=V-rescale
tc-grps=System
tau-t=0.1
ref-t=350
pcoupl=Parrinello-Rahman
pcoupltype=isotropic
tau-p=5.0
ref-p=1.0
compressibility=4.5e-5
constraints=h-bonds
cutoff-scheme=Verlet
