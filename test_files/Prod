rcoulomb=1.0
rvdw=1.0
pbc=xyz
DispCorr=EnerPres
nstxout-compressed=5000
nstenergy=5000
nstlog=5000


gmx grompp -f .\prod.mdp -c .\npt.gro -p .\topol.top -o .\prod.tpr 2> .\grompp_err.txt
gmx mdrun  -deffnm prod -gpu_id 0 -ntmpi 1
